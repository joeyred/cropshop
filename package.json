{
  "name": "cropshop",
  "version": "1.2.0",
  "private": true,
  "author": "Brian Hayes <bjoeyhayes@gmail.com>",
  "liscense": "MIT",
  "repository": "https://github.com/joeyred/cropshop",
  "bugs": "https://github.com/joeyred/cropshop/issues",
  "dependencies": {
    "@sentry/browser": "^5.5.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "filestack-js": "^2.0.7",
    "foundation-sites": "^6.5.3",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "micromodal": "^0.4.0",
    "react": "^16.8.6",
    "react-crouton": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-foundation": "^0.9.6",
    "react-icons": "^3.6.1",
    "react-image-crop": "^7.0.1",
    "react-redux": "^7.0.2",
    "react-responsive": "^7.0.0",
    "react-scripts": "2.1.8",
    "react-sizeme": "^2.6.7",
    "react-spinkit": "^3.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-sentry-middleware": "0.0.15",
    "redux-thunk": "^2.3.0",
    "simple-crypto-js": "^2.0.2",
    "source-map-explorer": "^2.0.1",
    "uniqid": "^5.0.3"
  },
  "scripts": {
    "start": "PORT=8000 react-scripts start",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build && npm run build-rename && npm run build-fix-sourcemap",
    "build-nomaps": "GENERATE_SOURCEMAP=false react-scripts build && npm run build-rename",
    "build-rename": "npm run build-rename-runtime-js && npm run build-rename-main-js && npm run build-rename-vendor-js && npm run build-rename-main-css && npm run build-rename-vendor-css",
    "build-rename-runtime-js": "renamer --find \"/runtime~main\\.[^\\.]+\\./i\" --replace \"runtime_cropshop_main_scripts.\" build/static/js/*",
    "build-rename-main-js": "renamer --find \"/main\\.[^\\.]+\\./i\" --replace \"cropshop_main_scripts.\" build/static/js/*",
    "build-rename-vendor-js": "renamer --find \"/2\\.[^\\.]+\\./i\" --replace \"cropshop_vendor_scripts.\" build/static/js/*",
    "build-rename-main-css": "renamer --find \"/main\\.[^\\.]+\\./i\" --replace \"cropshop_main_styles.\" build/static/css/*",
    "build-rename-vendor-css": "renamer --find \"/2\\.[^\\.]+\\./i\" --replace \"cropshop_vendor_styles.\" build/static/css/*",
    "build-fix-sourcemap": "npm run build-fix-sourcemap-js && npm run build-fix-sourcemap-css",
    "build-fix-sourcemap-js": "npm run build-fix-sourcemap-runtime-js && npm run build-fix-sourcemap-main-js && npm run build-fix-sourcemap-vendor-js",
    "build-fix-sourcemap-runtime-js": "replace-in-file \"/sourceMappingURL=runtime~main\\.[^\\.]+\\.js\\.map/i\" \"sourceMappingURL=runtime_cropshop_main_scripts.js.map\" build/static/js/runtime_cropshop_main_scripts.js --isRegex",
    "build-fix-sourcemap-main-js": "replace-in-file \"/sourceMappingURL=main\\.[^\\.]+\\.chunk.js\\.map/i\" \"sourceMappingURL=cropshop_main_scripts.chunk.js.map\" build/static/js/cropshop_main_scripts.chunk.js --isRegex",
    "build-fix-sourcemap-vendor-js": "replace-in-file \"/sourceMappingURL=2\\.[^\\.]+\\.chunk.js\\.map/i\" \"sourceMappingURL=cropshop_vendor_scripts.chunk.js.map\" build/static/js/cropshop_vendor_scripts.chunk.js --isRegex",
    "build-fix-sourcemap-css": "npm run build-fix-sourcemap-main-css && npm run build-fix-sourcemap-vendor-css",
    "build-fix-sourcemap-main-css": "replace-in-file \"/sourceMappingURL=main\\.[^\\.]+\\.chunk.css\\.map/i\" \"sourceMappingURL=cropshop_main_styles.chunk.css.map\" build/static/css/cropshop_main_styles.chunk.css --isRegex",
    "build-fix-sourcemap-vendor-css": "replace-in-file \"/sourceMappingURL=2\\.[^\\.]+\\.chunk.css\\.map/i\" \"sourceMappingURL=cropshop_vendor_styles.chunk.css.map\" build/static/css/cropshop_vendor_styles.chunk.css --isRegex",
    "upload-sourcemaps": "sentry-cli releases --org brian-hayes --project cropshop files cropshop@$npm_package_version upload-sourcemaps ./build/static/js --rewrite --validate",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": [
    "> 5%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-links": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "babel-loader": "^8.0.5",
    "better-console": "^1.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "renamer": "^1.1.2",
    "replace-in-file": "^4.1.2",
    "require-dot-file": "^0.4.0",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.1"
  }
}
